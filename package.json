{
    "name": "voyanga-toolset",
    "version": "0.0.3",
    "dependencies": {
	"coffee-script": "1.4.0",
	"jasmine-node" : "1.5.0",
	"pegjs": "0.7.0",
	"less": "1.3.3",
	"watch-less": "0.0.3"
    },
    "scripts": {
	"start": "coffee -cbwo ./frontend/assets/v2/js/compiled ./frontend/assets/v2/coffee",
